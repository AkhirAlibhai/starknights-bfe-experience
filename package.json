{
	"name": "starknights-bfe-experience",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "run-p type-check \"build-only {@}\" --",
		"preview": "npm run build && wrangler dev",
		"build-only": "vite build",
		"type-check": "vue-tsc --build",
		"lint": "biome check --write --verbose",
		"deploy": "npm run build && wrangler deploy",
		"gen:types": "wrangler types",
		"gen:assets": "pwa-assets-generator",
		"gen:all": "run-p gen:types gen:assets"
	},
	"dependencies": {
		"@vueuse/core": "^13.5.0",
		"@vueuse/rxjs": "^13.5.0",
		"@vueuse/sound": "^2.1.3",
		"dexie": "^4.0.11",
		"notivue": "^2.4.5",
		"pinia": "^3.0.3",
		"rxjs": "^7.8.2",
		"vue": "^3.5.17",
		"vue-router": "^4.5.1",
		"vuetify": "^3.9.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.1.1",
		"@cloudflare/vite-plugin": "^1.9.4",
		"@iconify/json": "^2.2.358",
		"@tsconfig/node22": "^22.0.2",
		"@types/node": "^22.16.3",
		"@vitejs/plugin-vue": "^6.0.0",
		"@vue/compiler-sfc": "^3.5.17",
		"@vue/tsconfig": "^0.7.0",
		"npm-run-all2": "^8.0.4",
		"sass-embedded": "^1.89.2",
		"typescript": "~5.8.3",
		"unplugin-auto-import": "^19.3.0",
		"unplugin-icons": "^22.1.0",
		"unplugin-vue-components": "^28.8.0",
		"unplugin-vue-router": "^0.14.0",
		"vite": "^7.0.4",
		"vite-plugin-vue-devtools": "^7.7.7",
		"vite-plugin-vue-layouts-next": "^1.0.0",
		"vite-plugin-vuetify": "^2.1.1",
		"vue-tsc": "^2.2.12",
		"wrangler": "^4.24.3"
	}
}
